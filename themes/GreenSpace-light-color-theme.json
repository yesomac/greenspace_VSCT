			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#8f8501"
			}
		},
		{
			"name": "HTML: Entities",
			"scope": [
				"constant.character.entity",
				"punctuation.definition.entity"
			],
			"settings": {
				"foreground": "#7e3d05"
			}
		},
		{
			"name": "CSS: Selectors",
			"scope": [
				"meta.selector",
				"meta.selector entity",
				"meta.selector entity punctuation",
				"entity.name.tag.css"
			],
			"settings": {
				"foreground": "#0a9739",
				"fontStyle": "italic bold"
			}
		},
		{
			"name": "CSS: Property Names",
